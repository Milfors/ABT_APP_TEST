BoxLayout:
    orientation: 'vertical'

    MDToolbar:
        title: 'ABT-Professional'

    MDBottomNavigation:

            #Нижння часть эакрана "Главная"
        MDBottomNavigationItem:
            name: 'Items'
            text: 'Главная'
            icon: 'home'

            FitImage:
                source: 'image/fon-2-sekcii-1.jpg'

            ScrollView:
                size: self.size
                MDBoxLayout:
                    orientation: 'vertical'
                    pos_hint: {'center_y':0.5}
                    adaptive_height: True
                    spacing: 2
                    padding: 30

                    MDRaisedButton:
                        text: 'Основы'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.Bases().open()

                    MDRaisedButton:
                        text: 'Файлы тонкая основа'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.Files().open()

                    MDRaisedButton:
                        text: 'Файлы педикюр тонкая основа'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.PedicureFiles().open()

                    MDRaisedButton:
                        text: 'Файлы вспененная основа'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.FilesPena().open()

                    MDRaisedButton:
                        text: 'Файлы педикюр вспененная основа'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.PedicureFilesPena().open()

                    MDRaisedButton:
                        text: 'Бафы'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.Bafi().open()

                    MDRaisedButton:
                        text: 'Другое'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_release: Factory.Other().open()

                    MDRaisedButton:
                        text: 'Очистить корзину'
                        pos_hint: {"center_x":.5}
                        width: dp(250) + (self.ids.lbl_txt.texture_size[0] - self.ids.lbl_txt.texture_size[0])
                        on_press: app.clearBasket()
                        md_bg_color : [1,0,0,1]



        #Нижняя часть главного экрана "Корзина"
        MDBottomNavigationItem:
            name: 'Basket'
            text: 'Корзина'
            icon: 'basket'

            FitImage:
                source: 'image/fon-2-sekcii-1.jpg'

            ScrollView:
                BoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    center: root.width / 1, root.height / 1
                    id: basketItems
                    spacing: 4

