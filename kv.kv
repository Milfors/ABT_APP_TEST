#Всплывающее окно Основ
<Bases@Popup>
    auto_dismiss: True
    title: 'Основы'

    background_normal: ""
    background_color: (26/255, 149/255, 184/255, .9)

    ScrollView:
        size: self.size

        GridLayout:

            size_hint_y: None
            height: self.minimum_height
            width: self.minimum_width

            cols: 1
            spacing: 5

